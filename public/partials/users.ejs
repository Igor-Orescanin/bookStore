


<div class="m-2">
    <form action="/users/addUser" method="GET">
      <button class="btn btn-success">Add User</button>
    </form>
    <div class="row justify-content-between">
      <%data.forEach(e=>{%>
      <div class="card m-2" style="width: 18rem">
        <div class="card-header">User : <%= e.fName %> <%= e.lName %></div>
        <form action="/users/<%= e._id%>" method="GET">
            <button class="btn btn-secondary w-100">Show User</button>
          </form>
      </div>

      <%})%>
    </div>
</div>
